{
  "name": "aipithet",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "paraglide-compile": "paraglide-js compile --project ./project.inlang --outdir ./src/i18n/generated",
    "dev": "next dev -p 8888 & pnpm paraglide-compile --watch",
